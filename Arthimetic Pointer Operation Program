// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int array[]={6,51,21,7,9,132,71,22,123,4};
    int *p1;
    int *p2;
    int i;
    p1=&array;
    p2=&array[4];
    printf("\n");
    printf("\n %u \t %d",p1,*p1);
    p1=p1+4;
    printf("\n %u \t %d",p1,*p1);
    p1++;
    printf("\n %u \t %d",p1,*p1);
    p1--;
    printf("\n %u \t %d",p1,*p1);
    p1=p1-4;
    printf("\n %u \t %d",p1,*p1);
    return 0;
}
