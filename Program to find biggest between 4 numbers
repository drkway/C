// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int a,b,c,d;
    printf("Enter value of 4 numbers:");
    scanf("%d\n%d\n%d\n%d",&a,&b,&c,&d);
    if((a>=b)&&(a>=c)){
        if(a>=d){
            printf("A(%d) is the biggest number",a);
        }
        else{
            printf("D(%d) is the biggest number",d);
        }
    }
    else if(b>=c){
        if(b>=d){
            printf("B(%d) is the biggest number",b);
        }
        else{
            printf("D(%d) is the biggest number",d);
        }
    }
    else if(c>=d){
        printf("C(%d) is the biggest number",c);
    }
    else{
        printf("D(%d) is biggest number",d);
    }
    return 0;
}
