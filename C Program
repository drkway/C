// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int income,cost;
    float profit,loss;
    printf("Enter Income And Cost:");
    scanf("%d \n %d",&income,&cost);
    if (income>cost){
        profit=income-cost;
        printf("The profit is %f",profit);
    }
    else if (income==cost){
        printf("No Profit or Loss");
    }
    else {
        loss=income-cost;
        printf("The loss is %f",loss);
    }
    return 1;
}
